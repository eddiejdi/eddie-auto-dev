      "datasource": "${ds_pg}",
      "description": "Mapa neural interativo mostrando comunicação entre agents com filtros de tempo e seleção",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 15,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "edges": {
          "colorMode": "gradient",
          "mainStatUnit": "short",
          "field": "weight"
        },
        "layout": {
          "algorithm": "force",
          "attraction": 1,
          "repulsion": 12
        },
        "nodes": {
          "colorField": "group",
          "colorMode": "fixed",
          "mainStatField": "message_count",
          "mainStatUnit": "short",
          "sizeField": "message_count"
        }
      },
      "pluginVersion": "9.5.0",
      "targets": [
        {
          "datasource": "${ds_pg}",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Nodes (agents)\nWITH recent_messages AS (\n  SELECT source, target, timestamp\n  FROM messages\n  WHERE $__timeFilter(timestamp)\n    AND source ~ '${source:regex}'\n    AND target ~ '${target:regex}'\n)\nSELECT\n  source as id,\n  source as label,\n  CASE \n    WHEN source LIKE '%python%' OR source LIKE '%javascript%' OR source LIKE '%typescript%' \n         OR source LIKE '%go%' OR source LIKE '%rust%' OR source LIKE '%java%' \n         OR source LIKE '%csharp%' OR source LIKE '%php%' THEN 'language'\n    WHEN source LIKE '%coordinator%' THEN 'coordinator'\n    WHEN source LIKE '%diretor%' THEN 'director'\n    WHEN source LIKE '%telegram%' THEN 'interface'\n    WHEN source LIKE '%llm%' THEN 'llm'\n    ELSE 'other'\n  END as \"group\",\n  COUNT(*) as message_count,\n  MAX(timestamp) as last_interaction\nFROM recent_messages\nGROUP BY source\nORDER BY message_count DESC\nLIMIT 100\n\nUNION\n\nSELECT\n  target as id,\n  target as label,\n  CASE \n    WHEN target LIKE '%python%' OR target LIKE '%javascript%' OR target LIKE '%typescript%' \n         OR target LIKE '%go%' OR target LIKE '%rust%' OR target LIKE '%java%' \n         OR target LIKE '%csharp%' OR target LIKE '%php%' THEN 'language'\n    WHEN target LIKE '%coordinator%' THEN 'coordinator'\n    WHEN target LIKE '%diretor%' THEN 'director'\n    WHEN target LIKE '%telegram%' THEN 'interface'\n    WHEN target LIKE '%llm%' THEN 'llm'\n    ELSE 'other'\n  END as \"group\",\n  COUNT(*) as message_count,\n  MAX(timestamp) as last_interaction\nFROM recent_messages\nWHERE target != 'all'\nGROUP BY target\nORDER BY message_count DESC\nLIMIT 100",
          "refId": "Nodes"
        },
        {
          "datasource": "${ds_pg}",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Edges (connections)\nWITH recent_messages AS (\n  SELECT source, target, timestamp\n  FROM messages\n  WHERE $__timeFilter(timestamp)\n    AND source ~ '${source:regex}'\n    AND target ~ '${target:regex}'\n)\nSELECT\n  source as \"from\",\n  target as \"to\",\n  COUNT(*) as weight,\n  MAX(timestamp) as last_interaction\nFROM recent_messages\nWHERE target != 'all'\nGROUP BY source, target\nORDER BY weight DESC\nLIMIT 500",
          "refId": "Edges"
        }
      ],
      "title": "Agent Neural Network Map",
      "type": "nodeGraph"
    },
