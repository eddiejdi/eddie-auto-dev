    {
      "datasource": "PostgreSQL",
      "description": "Mapa neural interativo mostrando comunicação entre agents",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 16,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "edges": {
          "colorMode": "gradient",
          "mainStatUnit": "short",
          "field": "weight"
        },
        "layout": {
          "algorithm": "force",
          "attraction": 1,
          "repulsion": 10
        },
        "nodes": {
          "colorField": "group",
          "colorMode": "fixed",
          "mainStatField": "agent",
          "mainStatUnit": "short",
          "sizeField": "message_count"
        }
      },
      "pluginVersion": "9.5.0",
      "targets": [
        {
          "datasource": "PostgreSQL",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Nodes (agents)\nSELECT DISTINCT\n  source as id,\n  source as label,\n  CASE \n    WHEN source LIKE '%python%' OR source LIKE '%javascript%' OR source LIKE '%typescript%' \n         OR source LIKE '%go%' OR source LIKE '%rust%' OR source LIKE '%java%' \n         OR source LIKE '%csharp%' OR source LIKE '%php%' THEN 'language'\n    WHEN source LIKE '%coordinator%' THEN 'coordinator'\n    WHEN source LIKE '%diretor%' THEN 'director'\n    WHEN source LIKE '%telegram%' THEN 'interface'\n    WHEN source LIKE '%llm%' THEN 'llm'\n    ELSE 'other'\n  END as \"group\",\n  COUNT(*) as message_count\nFROM messages\nWHERE timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY source\n\nUNION\n\nSELECT DISTINCT\n  target as id,\n  target as label,\n  CASE \n    WHEN target LIKE '%python%' OR target LIKE '%javascript%' OR target LIKE '%typescript%' \n         OR target LIKE '%go%' OR target LIKE '%rust%' OR target LIKE '%java%' \n         OR target LIKE '%csharp%' OR target LIKE '%php%' THEN 'language'\n    WHEN target LIKE '%coordinator%' THEN 'coordinator'\n    WHEN target LIKE '%diretor%' THEN 'director'\n    WHEN target LIKE '%telegram%' THEN 'interface'\n    WHEN target LIKE '%llm%' THEN 'llm'\n    ELSE 'other'\n  END as \"group\",\n  COUNT(*) as message_count\nFROM messages\nWHERE timestamp > NOW() - INTERVAL '24 hours'\n  AND target != 'all'\nGROUP BY target",
          "refId": "Nodes",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        },
        {
          "datasource": "PostgreSQL",
          "format": "table",
          "group": [],
          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Edges (connections)\nSELECT \n  source as \"from\",\n  target as \"to\",\n  COUNT(*) as weight,\n  MAX(timestamp) as last_interaction\nFROM messages\nWHERE timestamp > NOW() - INTERVAL '24 hours'\n  AND target != 'all'\nGROUP BY source, target\nHAVING COUNT(*) > 0\nORDER BY weight DESC",
          "refId": "Edges",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Agent Neural Network Map",
      "type": "nodeGraph"
    },
