    {
      "id": 10,
      "type": "gauge",
      "title": "ğŸ”§ Self-Healing Status (Stall > 300s)",
      "gridPos": {"h": 4, "w": 12, "x": 0, "y": 0},
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "count(increase(selfhealing_restarts_total[5m])) > 0",
          "legendFormat": "Restarts (5m)",
          "refId": "A"
        },
        {
          "expr": "avg(time() - process_start_time_seconds) > 300",
          "legendFormat": "Stalled Services",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "âœ… Healthy", "color": "green"},
                "1": {"text": "âš ï¸ Stalled", "color": "orange"}
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "orange", "value": 1},
              {"color": "red", "value": 3}
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 5
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": ["lastNotNull"]
        }
      }
    },
